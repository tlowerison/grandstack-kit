{
  "name": "grandstack-kit-client",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf dist; tsc && NODE_ENV=production yarn webpack",
    "build:profile": "rm -rf dist; NODE_ENV=production PROFILE=true yarn webpack",
    "build+gqlgen": "cd ../api && yarn build:graphql && cd ../client && yarn gqlgen",
    "gqlgen": "graphql-codegen --config codegen.yaml && eslint --ext .ts --ext .tsx src/generated/graphql.tsx --fix",
    "lint": "eslint --ext .ts --ext .tsx src --fix",
    "prepack": "yarn build",
    "start": "NODE_ENV=development yarn webpack serve",
    "build:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/build-image.sh",
    "publish:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/publish-image.sh",
    "start:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/start-image.sh"
  },
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@tlowerison/common-components": "^1.0.9",
    "cross-fetch": "^3.0.6",
    "graphql": "^15.3.0",
    "preact": "^10.5.9",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/fragment-matcher": "^1.16.3",
    "@graphql-codegen/time": "^2.0.1",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/ramda": "^0.27.17",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sass": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-graphql-tag": "^3.1.0",
    "babel-plugin-ramda": "^2.0.0",
    "compression-webpack-plugin": "^7.1.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "react-refresh": "^0.9.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.3",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
