{
  "name": "@grandstack-kit/api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist; tsc && NODE_ENV=production yarn webpack",
    "build:profile": "rm -rf dist; NODE_ENV=production PROFILE=true yarn webpack",
    "gqlgen": "yarn build && rm db/schema.graphql; IS_GRAPHQL_BUILD=true node ./dist/main.js && cd ../client && yarn gqlgen && cd ../server",
    "prepack": "yarn build",
    "start": "(! (ls dist/main.js >> /dev/null 2>&1) && yarn build); node ./dist/main.js",
    "build:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/build-image.sh",
    "publish:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/publish-image.sh",
    "start:image": "SCRIPTS=\"$(cd $PWD && cd .. && echo \"$PWD/scripts\")\" && $SCRIPTS/start-image.sh"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "@tlowerison/neo4j-graphql-js": "^1.0.5",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.18.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "graphql": "^15.3.0",
    "helmet": "^4.3.1",
    "neo4j-driver": "^4.2.1",
    "ramda": "^0.27.1",
    "rate-limiter-flexible": "^2.1.15",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.5.5",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.11.2",
    "@types/ramda": "^0.27.19",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-ramda": "^2.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.3",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
